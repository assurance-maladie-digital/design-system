{
	"name": "@cnamts/eslint-config",
	"type": "module",
	"version": "2.0.0-bridge.0",
	"description": "ESLint configurations used across our projects",
	"author": {
		"name": "CNAM",
		"email": "digital_TL.cnam@assurance-maladie.fr"
	},
	"license": "MIT",
	"keywords": [
		"cnam",
		"lint",
		"eslint",
		"vue",
		"typescript",
		"javascript"
	],
	"exports": {
		".": {
			"import": "./dist/index.js",
			"require": "./dist/index.cjs"
		}
	},
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"files": ["dist"],
	"engines": {
		"node": ">=18.0.0"
	},
	"scripts": {
		"lint": "eslint .",
		"lint:fix": "eslint . --fix",
		"build": "tsup"
	},
	"peerDependencies": {
		"eslint": ">=8.0.0"
	},
	"dependencies": {
		"@cspell/eslint-plugin": "7.3.8",
		"@stylistic/eslint-plugin": "0.0.6",
		"@typescript-eslint/eslint-plugin": "6.7.4",
		"@typescript-eslint/parser": "6.7.4",
		"eslint-config-flat-gitignore": "0.1.0",
		"eslint-define-config": "1.24.1",
		"eslint-plugin-antfu": "1.0.0-beta.12",
		"eslint-plugin-case-police": "0.6.1",
		"eslint-plugin-i": "2.28.1",
		"eslint-plugin-jsdoc": "46.8.2",
		"eslint-plugin-jsonc": "2.10.0",
		"eslint-plugin-markdown": "3.0.1",
		"eslint-plugin-n": "16.1.0",
		"eslint-plugin-unicorn": "51.0.1",
		"eslint-plugin-unused-imports": "3.0.0",
		"eslint-plugin-vitest": "0.3.2",
		"eslint-plugin-vue": "9.17.0",
		"eslint-plugin-yml": "1.10.0",
		"globals": "13.23.0",
		"jsonc-eslint-parser": "2.3.0",
		"local-pkg": "0.5.0",
		"vue-eslint-parser": "9.3.2",
		"yaml-eslint-parser": "1.2.2"
	},
	"devDependencies": {
		"@cnamts/eslint-config": "workspace:*",
		"@eslint/js": "^8.57.0",
		"eslint": "8.57.0",
		"tsup": "8.0.2"
	},
	"publishConfig": {
		"access": "public"
	}
}
