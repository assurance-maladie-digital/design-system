{
	"name": "@cnamts/synapse-bridge",
	"type": "module",
	"version": "0.0.0",
	"description": "Portage of our components to Vue 3 without breaking changes",
	"author": {
		"name": "CNAM",
		"email": "digital_TL.cnam@assurance-maladie.fr"
	},
	"license": "MIT",
	"exports": {
		".": {
			"import": "./dist/synapse-bridge.js",
			"require": "./dist/synapse-bridge.umd.cjs"
		},
		"./style.css": "./dist/style.css"
	},
	"main": "./dist/synapse-bridge.umd.cjs",
	"module": "./dist/synapse-bridge.js",
	"types": "./dist/index.d.ts",
	"files": ["dist"],
	"engines": {
		"node": ">=18.0.0"
	},
	"scripts": {
		"dev:setup": "tsx scripts/devSetup.ts",
		"dev": "vite",
		"lint": "eslint",
		"test:unit": "vitest run",
		"preview": "vite preview",
		"analyze": "vite build --mode analyze",
		"build": "vue-tsc && vite build"
	},
	"peerDependencies": {
		"vue": "^3.3.0",
		"vuetify": "^3.3.0"
	},
	"dependencies": {
		"@cnamts/design-tokens": "2.12.0"
	},
	"devDependencies": {
		"@cnamts/eslint-config": "workspace:*",
		"@mdi/js": "7.2.96",
		"@vitejs/plugin-vue": "4.3.4",
		"@vitest/coverage-v8": "0.34.6",
		"@vue/test-utils": "2.4.1",
		"deepmerge": "4.3.1",
		"eslint": "8.52.0",
		"jest-serializer-vue": "3.1.0",
		"jsdom": "22.1.0",
		"resize-observer-polyfill": "1.5.1",
		"rollup-plugin-visualizer": "5.9.2",
		"sass": "1.67.0",
		"sass-loader": "13.3.2",
		"tsx": "3.13.0",
		"typescript": "5.0.4",
		"vite": "4.4.9",
		"vite-plugin-dts": "3.6.0",
		"vite-plugin-vuetify": "1.0.2",
		"vitest": "0.34.6",
		"vue-tsc": "1.8.11"
	},
	"publishConfig": {
		"access": "public"
	}
}
